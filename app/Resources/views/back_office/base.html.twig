        <aside class="sidebar-left">
           {{ include('back_office/include/menu.html.twig') }}
        </aside>
    </div>
    <!--left-fixed -navigation-->

    <!-- header-starts -->
   {{ include('back_office/include/header.html.twig') }}
    <!-- //header-ends -->


    {% block body %}

    {% endblock %}

    <!--footer-->
   {{ include('back_office/include/footer.html.twig') }}
    <!--//footer-->
</div>
<!--script-->
{{ include('back_office/include/script.html.twig') }}
<!--//script-->

</body>

</html>